{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"clean": {
			"cache": false
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"test": {
			"dependsOn": ["^test"],
			"outputs": ["coverage/**"]
		},
		"format": {
			"outputs": []
		},
		"format:staged": {
			"outputs": []
		},
		"lint": {
			"outputs": []
		}
	}
}

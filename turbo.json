{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["packages/svelte-reveal/dist/**"]
    },
    "format": {
      "outputs": []
    },
    "format:staged": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    }
  }
}
